    color:rgba(0,0,0,0.4);
}
.my-weui-cell .my-notice{
    color:#F55231;
    font-weight:bold ;
}
/*----------------------------------------- 让座信息样式----------------------------------------*/
.seatRush-zone{
    position:relative;
    margin:0 -20px;
    border-top:1rpx solid #e6e6ea;
}
.seatRush-zone .my-seat-cell{
    padding:10px 15px;
    position:relative;
    display:-webkit-box;
    display:-webkit-flex;
    display:flex;
    -webkit-box-align:center;
    -webkit-align-items:center;
    align-items:center;
    color: #000;
}
.my-seat-cell .my-input-seat{
    width: 150px;
}
.my-weui-cell .my-input-seat-code{
    width: 60px;
}
.seatRush-zone .my-cancel-seat{
    position: absolute;
    top:-80rpx;
    right:15rpx;

}
.my-cancel-seat .my-cancel-img{
    width: 40rpx;
    height: 40rpx;
}
.seat-out{
    filter: grayscale(100%);
}